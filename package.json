{
  "name": "sendit",
  "version": "1.0.0",
  "description": "Andela Developer Challenge SendIT",
  "engines": {
    "node": "10.9.0"
  },
  "scripts": {
    "test": " npm run build && mocha dist/test/*.js --exit",
    "build": "npm run clear && babel ./ --out -d dist --ignore node_modules,UI",
    "clear": "rimraf dist coverage",
    "start": "nodemon app.js --exec babel-node --presets env",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ../dist/test/*.js --exit",
    "coverage": "npm run cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyedejipeace/SendIT.git"
  },
  "keywords": [
    "node.js",
    "Express",
    "ESlint",
    "babel"
  ],
  "author": "Oyedeji Peace <oyedejipeace@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@types/swagger-ui-express": "3.0.0",
    "ajv": "^6.5.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-validator": "5.3.0",
    "jsonwebtoken": "8.3.0",
    "morgan": "1.9.0",
    "nodemon": "^1.18.5",
    "path": "0.12.7",
    "pg": "7.5.0",
    "rimraf": "2.6.2",
    "swagger-ui-express": "4.0.1",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "node-dev": "3.1.3",
    "supertest": "^3.3.0"
  },
  "bugs": {
    "url": "https://github.com/oyedejipeace/SendIT/issues"
  },
  "homepage": "https://github.com/oyedejipeace/SendIT#readme",
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  }
}
