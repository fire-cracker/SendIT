{
  "name": "sendit",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/oyedejipeace/SendIT.svg?branch=develop)](https://travis-ci.org/oyedejipeace/SendIT)[![Coverage Status](https://coveralls.io/repos/github/oyedejipeace/SendIT/badge.svg?branch=develop)](https://coveralls.io/github/oyedejipeace/SendIT?branch=develop)[![Code Climate](https://codeclimate.com/github/codeclimate/codeclimate/badges/gpa.svg)](https://codeclimate.com/github/oyedejipeace/SendIT)![GitHub](https://img.shields.io/github/license/mashape/apistatus.svg)",
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": "10.9.0"
  },
  "scripts": {
    "local": "nodemon app.js --exec babel-node --presets env",
    "test": " npm run build && mocha dist/test/*.js --exit",
    "start": "npm run build && node ./dist/app.js",
    "build": "npm run clear && babel ./ --out -d dist --ignore node_modules,UI",
    "clear": "rimraf dist coverage",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -- -R spec ./dist/test/*.js --exit",
    "coverage": "npm run cover"
  },
  "keywords": [
    "node.js",
    "Express",
    "ESlint",
    "babel"
  ],
  "dependencies": {
    "@types/swagger-ui-express": "3.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "jsonwebtoken": "8.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.6",
    "path": "0.12.7",
    "pg": "7.5.0",
    "rimraf": "^2.6.2",
    "swagger-ui-express": "4.0.1",
    "validator": "^10.9.0",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "chai": "^4.1.2",
    "coveralls": "3.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.14.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "node-dev": "^3.1.3",
    "supertest": "3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oyedejipeace/SendIT.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oyedejipeace/SendIT/issues"
  },
  "homepage": "https://github.com/oyedejipeace/SendIT#readme"
}
